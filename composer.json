{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "^7.0",
        "illuminate/support": "~7.0",
	"guzzlehttp/guzzle": "~6.0",
	"maatwebsite/excel": "~3.1.0",
	"intervention/image": "^2.3",
	"darryldecode/cart": "dev-master",
	"unlu/laravel-api-query-builder": "~1.0",
	"bugsnag/bugsnag-laravel": "^2.0",
        "laravel/passport": "^8.0",
        "predis/predis": "^1.1",
        "laravel/scout": "^7.0",
        "algolia/algoliasearch-client-php": "^1.16",
        "laravel/cashier": "~11.0",
        "stripe/stripe-php": "7.*",
        "league/flysystem-aws-s3-v3": "~1.0",
        "dusterio/laravel-aws-worker": "^0.1.11",
        "swiftmailer/swiftmailer": "~6.0",
        "google/apiclient": "^2.0",
        "fideloper/proxy": "^4.1",
        "phpoffice/phpspreadsheet": "^1.6",
        "rinvex/laravel-bookings": "^4.0",
        "yab/laravel-scout-mysql-driver": "^2.3",
        "opentok/opentok": "^4.4",
        "mercadopago/dx-php": "dev-master",
        "laravel/socialite": "^4.3",
        "laravel/envoy": "^2.2",
        "laravel/ui": "^2.0",
        "fruitcake/laravel-cors": "^2.0",
        "laravel/helpers": "^1.2",
        "symfony/psr-http-message-bridge": "^2.0",
        "nyholm/psr7": "^1.3",
        "grimzy/laravel-mysql-spatial": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^8.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "filp/whoops" :"~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
